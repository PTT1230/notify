<body id="bodyindex" >
  <div id="index_top">
  <!-- ローディング画面 -->
  <div id="loading">
    <div class="spinner"></div>
  </div>
    <% unless user_signed_in? %>
      <div class="user_out">
        <%= link_to 'Log In', new_user_session_path, class: 'user_link' %>
        <%= link_to 'Sign Up', new_user_registration_path, class: 'user_link' %>
      </div>
    <% else %>
      <%= render 'content' %>
    <% end %>
  </div>
</body>